{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://odin-book-front.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      },
      "dest": "/",
      "methods": ["get", "post", "put", "patch", "options", "delete"]
    }
  ]
}
