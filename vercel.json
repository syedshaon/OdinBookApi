{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": {
        "Access-Control-Allow-Origin": "https://odin-book-front.vercel.app",
        "Access-Control-Allow-Credentials": "true",
        "preflightContinue": "true"
      }
    }
  ]
}
